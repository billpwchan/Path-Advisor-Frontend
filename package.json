{
  "name": "pathadvisor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "env-cmd": "^8.0.2",
    "lodash.compact": "^3.0.1",
    "lodash.get": "^4.4.2",
    "lodash.isnil": "^4.0.0",
    "lodash.pick": "^4.4.0",
    "lodash.throttle": "^4.1.1",
    "lodash.uniq": "^4.5.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.4.1",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.3",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "build:analyze": "react-scripts build",
    "build:staging:dev": "env-cmd .env.staging-dev npm run build",
    "build:staging:s3": "env-cmd .env.staging-s3 npm run build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && ./deploy.sh",
    "deploy:staging:dev": "npm run build:staging:dev && bash ./deploy-staging-dev.sh",
    "deploy:staging:s3": "npm run build:staging:s3 && bash ./deploy-staging-s3.sh",
    "deploy:doc:staging:dev": "bash ./deploy-doc-staging-dev.sh",
    "deploy:doc:staging:s3": "bash ./deploy-doc-staging-s3.sh",
    "cloneEnv": "bash ./cloneEnv.sh"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "docsify-cli": "^4.2.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "prettier-eslint": "^8.8.2",
    "source-map-explorer": "^1.6.0"
  },
  "browserslist": {
    "development": [
      ">0.25%",
      "not op_mini all",
      "ie 9"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 9"
    ]
  },
  "bolt": {
    "workspaces": [
      "src/plugins/*"
    ]
  }
}
